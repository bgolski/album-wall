{
  "name": "vinyl-wall",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:static": "cross-env NEXT_STATIC_EXPORT=true next build",
    "build:ssr": "cross-env NEXT_STATIC_EXPORT=false next build",
    "start": "serve out -s -p 3001",
    "start:next": "next start --port 3001",
    "lint": "next lint",
    "lint:fix": "next lint --fix && prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "export": "next build",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "fix-all": "npm run lint:fix && npm run type-check",
    "prepare": "husky",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "axios": "^1.7.9",
    "disconnect": "^1.2.2",
    "html2canvas": "^1.4.1",
    "next": "^15.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/html2canvas": "^0.5.35",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.1.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": [
      "tsc --noEmit"
    ]
  }
}
